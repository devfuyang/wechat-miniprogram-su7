page {
  margin: 0;
  padding: 0;
}
/* 容器样式 */
.container {
  position: relative;
  width: 100%;
  height: 100vh;
  margin: 0;
  padding: 0;
}

/* 地图铺满屏幕 */
.full-screen-map {
  width: 100%;
  height: 100vh;
}

/* 卡片样式 */
.card {
  position: absolute;
  bottom: 20px;
  left: 20px;
  right: 20px;
  background-color: rgb(255, 255, 255);
  border-radius: 10px;
  padding: 15px;
  box-shadow: 0 0 10px rgba(0, 0, 0, 0.2);
}
